if event.type == "item" then
   local s = event.item.name
   if s == "technic:walking_tractor_1"
      or s == "technic:walking_tracor_2"
      or s == "technic:walking_tracor_3"
      or s == "technic:walking_tracor_4"
      or s == "technic:walking_tracor_5"
      or s == "technic:walking_tracor_6"
      or s == "technic:planter_1"
      or s == "technic:planter_2"
      or s == "technic:planter_3"
      or s == "technic:planter_4"
      or s == "technic:planter_5"
      or s == "technic:planter_6"
      or s == "technic:sonic_screwdriver"
      or s == "technic:prospector_mk2"
      or s == "technic:laser_mk1"
      or s == "technic:laser_mk2"
      or s == "technic:laser_mk3"
      or s == "technic:mining_drill_mk3_1"
      or s == "technic:mining_drill_mk3_2"
      or s == "technic:mining_drill_mk3_3"
      or s == "technic:mining_drill_mk3_4"
      or s == "technic:mining_drill_mk3_5"
      or s == "powerbanks:powerbank_mk1"
      or s == "powerbanks:powerbank_mk2"
      or s == "powerbanks:powerbank_mk3"
      or s == "technic:vacuum"
      or s == "technic:chainsaw"
      or s == "technic:chainsaw_mini"
      or s == "technic:chainsaw_mk2"
      or s == "technic:spray_painter_1"
      or s == "technic:spray_painter_2"
      or s == "technic:spray_painter_3"
      or s == "technic:spray_painter_4"
      or s == "technic:spray_painter_5"
      or s == "technic:spray_painter_6"
      or s == "technic:spray_painter_7"
      or s == "technic:spray_painter_8"
      or s == "technic:spray_painter_9"
      or s == "technic:fluorescent:spray_painter_1"
      or s == "technic:fluorescent:spray_painter_2"
      or s == "technic:fluorescent:spray_painter_3"
      or s == "technic:fluorescent:spray_painter_4"
      or s == "technic:fluorescent:spray_painter_5"
      or s == "technic:fluorescent:spray_painter_6"
      or s == "technic:fluorescent:spray_painter_7"
      or s == "technic:fluorescent:spray_painter_8"
      or s == "technic:fluorescent:spray_painter_9"
  then
      return "red" --This goes to the battery box.
  end
   if s == "staffs:growstaff"
      or s == "staffs:colorstaff"
      or s == "staffs:mysterystaff"
      or s == "staffs:humiditystaff"
      or s == "ethereal:light_staff"
      or s == "ethereal:crystal_gilly_staff"
      or s == "froststaff:staff"
      or s == "drawers:trolley"
      or s == "xtraores:spear_gungir"
      or s == "hangglider:hangglider"
      or s == "cottages:hammar"
      or s == "mobs:lasso"
      or s == "xtraores:drill_geminitinum"
      or s == "multitools:multitool_crystal"
      or s == "multitools:multitool_mithril"
      or s == "multitools:multitool:lava"
      or s == "xtraores:sword_geminitinum"
      or s == "xtraores:spear_geminitinum"
  then
      return "black" --This goes to the tool workshop.
   end
end
